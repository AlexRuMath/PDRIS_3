pipeline {
    agent any

    tools {
        nodejs "node"
    }

    stages{
        stage('Cloning'){
            steps {
                git "https://github.com/AlexRuMath/PDRIS_3.git/refs/heads/main"
            }
        }

        stage("Install dependencies"){
            steps {
                sh 'npm install'
            }
        }

        stage("Test"){
            steps {
                sh 'node test'
            }
        }
    }
}